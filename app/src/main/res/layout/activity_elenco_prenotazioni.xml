<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sfondo_activity"
    android:orientation="vertical"
    tools:context=".prenotazione.ElencoPrenotazioni">

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/indietro" />

    <TextView
        android:id="@+id/textViewPrenotazioneTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingLeft="15dp"
        android:text="Prenotazioni"
        android:textColor="@color/black"
        android:textSize="20dp" />

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tabPrenotazioni"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/quantum_white_divider">
    </com.google.android.material.tabs.TabLayout>

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/viewPagerPrenotazioni"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>