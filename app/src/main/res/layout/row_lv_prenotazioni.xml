<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:id="@+id/tv_tipoPrenotazione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp"
        android:textStyle="bold">
    </TextView>
    <TextView
        android:id="@+id/tv_dataPrenotazione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp">
    </TextView>
    <TextView
        android:id="@+id/tv_nomeAnnuncioPrenotazione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp">
    </TextView>
    <TextView
        android:id="@+id/tv_utentePrenotazione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp">
    </TextView>
    <TextView
        android:id="@+id/tv_emailPrenotazione"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp">
    </TextView>
    <TextView
        android:id="@+id/tv_daPagare"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="5dp">
    </TextView>
</LinearLayout>